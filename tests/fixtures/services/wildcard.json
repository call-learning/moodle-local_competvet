  "description": "Allows any service to authenticate via CAS",
  "evaluationOrder": 1,
  "attributeReleasePolicy": {
    "@class": "org.apereo.cas.services.ReturnAllowedAttributeReleasePolicy",
    "allowedAttributes": ["java.util.ArrayList", ["email", "displayName", "firtname", "lastname"]]
  }
}
